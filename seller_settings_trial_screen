--- [ seller_settings_skin_theme_light_button ]   [ button    ]    "–°–í–ï–¢–õ–ê–Ø –¢–ï–ú–ê"   [ color2 ]       [ color1 / color2 ]   [ yes/no ]     [ center ]
--- [ seller_settings_skin_theme_gray_button  ]   [ button    ]    "–°–ï–†–ê–Ø –¢–ï–ú–ê"     [ color1 ]       [ color1 / color2 ]   [ yes/no ]     [ center ]
--- [ seller_settings_skin_theme_dark_button  ]   [ button    ]    "–¢–Å–ú–ù–ê–Ø –¢–ï–ú–ê"    [ color2 ]       [ color1 / color2 ]   [ yes/no ]     [ center ]
s4) [ seller_settings_skin_custom             ]   —Å–µ–∫—Ü–∏—è –∏–∑ 4 —ç–ª–µ–º–µ–Ω—Ç–æ–≤ —Ä–∞—Å–ø–æ–ª–æ–∂–µ–Ω–Ω—ã—Ö —Å–≤–µ—Ä—Ö—É-–≤–Ω–∏–∑ –≤ —Å–ª–µ–¥—É—é—â–µ–º –ø–æ—Ä—è–¥–∫–µ:
--- [ seller_settings_skin_custom_header      ]   [ header    ]                                                                           [ center ]
--- [ seller_settings_skin_custom_button      ]   [ button    ]    "–ì–ê–õ–ï–†–ï–Ø"        [ color2 ]       [ color1 / color2 ]   [ no     ]     [ center ]
